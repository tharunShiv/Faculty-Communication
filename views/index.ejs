<body>
  <% include partials/layout.ejs %>
    <section id="showcase">
        <div class="container">
            <h1>D S C E</h1>
            <pre>
                <p> Dayananda Sagar College Of Engineering</p>
                </pre>
        </div>
    </section><br/>

    <section id="login">
        <div class="container">
            <form method="post" action="/users/index">
                <fieldset id="loginside">
                    <legend>Login/SignUp</legend>
                    <br/>
                    <input class="ifield" type="text" autofocus name="username" placeholder="Username"><br/><br/>
                    <input class="ifield" type="password" name="password" placeholder="Password"><br/><br/>
                    <button id="log" type="submit" name="login">Login</button>
                    <button id="sign" type="reset" target="/users/register">Reset</button><br/><br/>
                </fieldset>
            </form>
        </div>

        <% include partials/error.ejs %>

    </section>
